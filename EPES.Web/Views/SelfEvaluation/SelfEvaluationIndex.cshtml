@model IEnumerable<SelfEvaluationDto>

<div class="container-fluid">
    <div class="card shadow border-0">
        <div class="card-header bg-secondary bg-gradient">
            <div class="row">
                <div class="col-12 text-center">
                    <h1 class="text-white">Self-Evaluated data</h1>
                </div>
            </div>
        </div>
        <!-- <div class="card-body p-4">
            <div class="row pb-3">
                <div class="col-6">
                </div>
            </div>
        </div> -->
        <table class="table table-striped table-bordered">
            <thead>
                <tr>
                    <th>
                        Employee Email
                    </th>
                    <th>
                        Submission Date
                    </th>
                    <th>
                        Task Completed
                    </th>
                    <th>
                        Technical
                    </th>
                    <th>
                        Communication
                    </th>
                    <th>
                        Adaptability
                    </th>
                    <th>
                        Time Management
                    </th>
                    <th>
                        Goal Achievement
                    </th>
                    <th>
                        Manager Evaluation
                    </th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>
                            @item.EmployeeEmail
                        </td>
                        <td>
                            @item.SubmissionDate
                        </td>
                        <td>
                            @item.TaskCompleted
                        </td>
                        <td>
                            @item.Technical
                        </td>
                        <td>
                            @item.Communication
                        </td>
                        <td>
                            @item.Adaptability
                        </td>
                        <td>
                            @item.TimeManagement
                        </td>
                        <td>
                            @item.GoalAchievement
                        </td>
                        <!-- Add more <td> elements for additional properties as needed -->
                        <td>
                            <!-- Button for Manager Evaluation -->
                            <a asp-controller="ManagerEvaluation" asp-action="CreateManagerEvaluation" asp-route-id="@item.EmployeeEmail" class="btn btn-success">Manager Evaluation</a>
                        </td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>
