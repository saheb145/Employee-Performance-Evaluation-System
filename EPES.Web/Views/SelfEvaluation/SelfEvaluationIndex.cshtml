@model IEnumerable<SelfEvaluationDto>

<div class="card shadow border-0 mt-4">
	<div class="card-header bg-secondary bg-gradient ml-0 py-3">
		<div class="row">
			<div class="col-12 text-center">
				<h1 class="text-white">SelfEvaluation data</h1>
			</div>
		</div>
	</div>
	<div class="card-body p-4">
		<div class="row pb-3">
			<div class="col-6">
			</div>
			<div class="col-6 text-end">
				<a asp-action="CreateSelfEvaluation" class="btn btn-outline-primary"><i class="bi bi-plus-square"></i> Create new SelfEvaluation</a>
			</div>
		</div>
		<table class="table">
			<thead>
				<tr>
					<th>
						Id
					</th>
					<th>
						ApplicationUserDtoEmail
					</th>
					<th>
						SubmissionDate
					</th>
					<th>
						TaskCompleted
					</th>
					<th>
						Technical
					</th>
					<th>
						Commmunication
					</th>
					<th>
						Adaptability
					</th>
					<th>
						TimeManagement
					</th>
					<th>
						GoalAchievement
					</th>
					<!-- Add more columns for additional properties as needed -->
					<th></th>
				</tr>
			</thead>
			<tbody>
				@foreach (var item in Model)
				{
					<tr>
						<td>
							@item.Id
						</td>
						<td>
							@item.ApplicationUserDtoEmail
						</td>
						<td>
							@item.SubmissionDate
						</td>
						<td>
							@item.TaskCompleted
						</td>
						<td>
							@item.Technical
						</td>
						<td>
							@item.Commmunication
						</td>
						<td>
							@item.Adaptability
						</td>
						<td>
							@item.TimeManagement
						</td>
						<td>
							@item.GoalAchievement
						</td>
						<!-- Add more <td> elements for additional properties as needed -->
						<td>
							@* <a asp-action="DeleteSelfEvaluation" asp-route-Id="@item.Id" class="btn btn-danger">
								<i class="bi bi-trash"></i>
							</a> *@
						</td>
					</tr>
				}
			</tbody>
		</table>
	</div>
</div>
