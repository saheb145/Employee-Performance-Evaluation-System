@model IEnumerable<ManagerEvaluationDto>

<div class="card shadow border-0 mt-4">
	<div class="card-header bg-secondary bg-gradient ml-0 py-3">
		<div class="row">
			<div class="col-12 text-center">
				<h1 class="text-white">ManagerEvaluation data</h1>
			</div>
		</div>
	</div>
	<div class="card-body p-4">
		<div class="row pb-3">
			<div class="col-6">
			</div>
			
		</div>
		<table class="table">
			<thead>
				<tr>
					
					<th>
						EmployeeEmail
					</th>
					<th>
						Remarks
					</th>
					<th>
						Score

					</th>
				</tr>
			</thead>
			<tbody>
				@foreach (var item in Model)
				{
					<tr>
						<td>
							@item.EmployeeEmail
						</td>
						<td>
							@item.Remarks
						</td>
						<td>
							@item.Score
						</td>
						
					</tr>
				}
			</tbody>
		</table>

	</div>

</div>