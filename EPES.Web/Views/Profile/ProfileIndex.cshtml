@model IEnumerable<UserDto>

<div class="card" style="margin-top:50px">
	<img src="data:image/jpeg;base64," alt="John" style="width:100%">
	@foreach (var item in Model)
	{
		<h1>@item.Name</h1>

		<p class="title">
			@item.Email
		</p>
		<p>@item.PhoneNumber</p>



		<a asp-controller="Auth" asp-action="Logout" asp-route-Id="@item.ID" class="btn btn-danger">Logout</a>



	}
</div>



<style>
	.card {
		box-shadow: 0 4px 8px 0 rgba(0, 0, 0, 0.2);
		max-width: 300px;
		margin: auto;
		text-align: center;
	}



	.title {
		color: grey;
		font-size: 18px;
	}



	button {
		border: none;
		outline: 0;
		display: inline-block;
		padding: 8px;
		color: white;
		background-color: #000;
		text-align: center;
		cursor: pointer;
		width: 100%;
		font-size: 18px;
	}



	a {
		text-decoration: none;
		font-size: 22px;
		color: black;
	}



		button:hover, a:hover {
			opacity: 0.7;
		}
</style>